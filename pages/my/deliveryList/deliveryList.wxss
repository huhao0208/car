.addresss_card{padding:35rpx;border-bottom:0.5rpx solid #EEE;font-size:24rpx;color:#000}.addresss_card .selected{position:absolute;right:40rpx;top:40rpx}.addresss_card .username{padding:20rpx 0}.addresss_card .add_editor{display:flex;justify-content:space-between}.cell_right,.cell_left{height:100%;width:65px;text-align:center;display:flex;align-items:center;justify-content:center;color:#FFF}.cell_left{background:#1AAD19}.cell_right{background:#D01A17}.addAddress_btn{position:fixed;background:#FFF;z-index:999;width:100%;bottom:0;padding:20rpx}.addAddress_btn button{margin:20rpx;background:#D01A17;color:#FFF}.nodata{width:100vw;height:100vh}.nodata .no-address{position:relative;margin-top:50%;transform:translateY(-50%);width:100vw;height:300rpx;box-sizing:border-box;background:url("http://lantin-xiaochengxu.oss-cn-hangzhou.aliyuncs.com/wechat/wushuju.png") no-repeat center;background-size:200rpx;font-size:24rpx;color:#CCC}.nodata .no-address::after{content:'请添加收货地址';display:block;width:100%;position:absolute;bottom:20rpx;left:0;text-align:center}
